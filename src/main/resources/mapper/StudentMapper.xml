<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.xiafan.dao.StudentMapper">
  
  <select id="selectAll"  resultType="cn.xiafan.entity.Student">
    select * from student
  </select>
  
  <insert id="insert" parameterType="Student">
  	insert into student(real_name,phone,email,password,student_id,college_professional_class) values (#{realName},#{phone},#{email},#{password},#{studentId},#{collegeProfessionalClass})
  </insert>
  
</mapper>