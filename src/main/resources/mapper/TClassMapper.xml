<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.xiafan.dao.TClassMapper">
  
  <select id="list"  resultType="TClass">
    select * from t_class
  </select>

  <insert id="insert" parameterType="TClass">
  	insert into t_class(college_id,professional_id) values (#{collegeId},#{professionalId})
  </insert>

</mapper>